{
  "name": "User A Chrome Extension",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_icon": "bath_logo.png",
    "default_title": "Click to open side panel"
  },
  "permissions": [
    "sidePanel",
    "activeTab",
    "tabs",
    "history",
    "notifications",
    "storage"
  ],
    "background": {
        "service_worker": "background.js"
  },
  "side_panel": {
    "default_path": "side_panel.html",
    "content_security_policy": {
      "script_src": "'self' https://cdn.tailwindcss.com",
      "object_src": "'self'"
    }
  },
  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
      }
  ]
}